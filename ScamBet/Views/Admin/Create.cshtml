@model ScamBet.Entitties.Admin

<form asp-action="Create">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="form-group">
        <label asp-for="Username" class="control-label"></label>
        <input asp-for="Username" class="form-control" />
        <span asp-validation-for="Username" class="text-danger"></span>
    </div>
    <!-- Reszta pól dla klasy Account -->
    <div class="form-group">
        <label asp-for="admin_rank" class="control-label"></label>
        <input asp-for="admin_rank" class="form-control" />
        <span asp-validation-for="admin_rank" class="text-danger"></span>
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>

